<% logger.debug "Loading users new stuff:" %>
<% logger.debug :user %>
<%= error_messages_for :user %>
<% form_for :user, :url => users_path do |f| %>
<% logger.debug "Just past form tag:" %>
<p><label for="login">First Name</label><br/>
<%= f.text_field :firstName %></p>

<p><label for="login">Last Name</label><br/>
<%= f.text_field :lastName %></p>

<p><label for="login">Login</label><br/>
<%= f.text_field :login %></p>

<p><label for="email">Email</label><br/>
<%= f.text_field :email %></p>

<% logger.debug "Middle of the form:" %>

<p><label for="password">Password</label><br/>
<%= f.password_field :password %></p>

<p><label for="password_confirmation">Confirm Password</label><br/>
<%= f.password_field :password_confirmation %></p>

<p><%= submit_tag 'Sign up' %></p>
<% logger.debug "End of form:" %>
<% end %>
<% logger.debug "Finished Loading users new stuff:" %>
